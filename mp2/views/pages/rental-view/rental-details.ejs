<%- include('../../fragments/header.ejs') %>
<section id="my-game-list" class="py-5">
    <main class="container">
        <h2 class="display-5">Lista wypożyczeń</h2>
        <form action="">
            <div class="form-group">
                <label for="idInstrument">IdInstrument: *</label>
                <input type="text" id="idInstrument" class="form-control" required>
            </div>
            <div class="form-group">
                <label for="idKlient">IdKlient: *</label>
                <input type="text" id="idKlient" class="form-control" required>
            </div>
            <div class="form-group">
                <label for="dataWypozyczenia">Data wypożyczenia: *</label>
                <input type="date" id="dataWypozyczenia" class="form-control" required>
            </div>
            <div class="form-group">
                <label for="dataZwrotu">Data zwrotu: * </label>
                <input type="date" id="dataZwrotu" class="form-control">
            </div>
            <div class="row">
                <div class="col-md-4">
                    <button type="submit" class="btn btn-primary btn-lg w-100 mt-4">Edytuj</button>
                </div>
            </div>
        </form>
        <a href="/rentals" class="btn btn-dark mt-2">Wróć</a>
    </main>
</section>
<%- include('../../fragments/footer.ejs') %>